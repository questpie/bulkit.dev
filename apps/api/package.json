{
  "name": "@bulkit/api",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --bun --watch src/main.entry.ts",
    "dev:worker": "bun run --bun --watch src/worker.entry.ts",
    "dev:api": "bun run --bun --watch src/api.entry.ts",
    "build": "bun build ./src/main.entry.ts --target bun --outfile out/main.entry.js --minify",
    "build:api": "bun build ./src/server.entry.ts --target bun --outfile out/server.entry.js --minify",
    "build:worker": "bun build ./src/worker.entry.ts --target bun --outfile out/worker.entry.js --minify",
    "prod": "bun run out/main.entry.js",
    "prod:api": "bun run out/server.entry.js",
    "prod:worker": "bun run out/worker.entry.js",
    "test": "bun test",
    "types:test": "tsc --noEmit test/**/*.ts",
    "types:src": "tsc --noEmit src/**/*.ts",
    "db:push": "bun drizzle-kit push",
    "db:studio": "bun drizzle-kit studio",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.53",
    "@ai-sdk/openai": "^0.0.70",
    "@aws-sdk/client-s3": "^3.679.0",
    "@aws-sdk/s3-request-presigner": "^3.679.0",
    "@bogeychan/elysia-logger": "^0.1.4",
    "@bulkit/jobs": "workspace:*",
    "@bulkit/mail": "workspace:*",
    "@bulkit/redis": "workspace:*",
    "@bulkit/seed": "workspace:*",
    "@bulkit/shared": "workspace:*",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/swagger": "^1.1.5",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@paralleldrive/cuid2": "paralleldrive/cuid2",
    "@react-email/render": "^1.0.1",
    "@sinclair/typebox": "0.32.34",
    "ai": "^3.4.25",
    "arctic": "^2.0.1",
    "drizzle-orm": "^0.35.3",
    "drizzle-typebox": "^0.1.1",
    "elysia": "^1.1.23",
    "elysia-http-error": "^1.3.0",
    "elysia-ip": "^1.0.7",
    "flydrive": "^1.1.0",
    "geoip-lite": "^1.4.10",
    "google-auth-library": "^9.14.2",
    "googleapis": "^144.0.0",
    "lucia": "^3.2.2",
    "nodemailer": "^6.9.16",
    "open-graph-scraper": "^6.8.2",
    "oslo": "^1.2.1",
    "pino-pretty": "^11.3.0",
    "postgres": "^3.4.5",
    "pusher": "^5.2.0",
    "react": "^19.0.0-rc-02c0e824-20241028",
    "resend": "^4.0.0",
    "twitter-api-v2": "^1.18.1",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.1.3",
    "@types/bun": "latest",
    "@types/nodemailer": "^6.4.16",
    "@types/geoip-lite": "^1.4.4",
    "@types/ua-parser-js": "^0.7.39",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.26.2"
  },
  "module": "src/index.js"
}
