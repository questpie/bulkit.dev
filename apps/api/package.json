{
  "name": "@bulkit/api",
  "version": "1.0.50",
  "type": "module",
  "scripts": {
    "dev": "RUNTIME_MODE=all bun run --bun --watch src/main.entry.ts",
    "dev:worker": "RUNTIME_MODE=worker bun run --bun --watch src/main.entry.ts",
    "dev:api": "RUNTIME_MODE=api bun run --bun --watch src/main.entry.ts",
    "prod": "bun run src/server.entry.ts",
    "prod:worker": "bun run src/worker.entry.ts",
    "test": "bun test",
    "types:test": "tsc --noEmit test/**/*.ts",
    "types:src": "tsc --noEmit src/**/*.ts",
    "db:push": "bun drizzle-kit push",
    "db:studio": "bun drizzle-kit studio",
    "db:generate": "bun drizzle-kit generate",
    "db:migrate": "bun drizzle-kit migrate"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.651.1",
    "@aws-sdk/s3-request-presigner": "^3.651.1",
    "@bulkit/jobs": "workspace:*",
    "@bulkit/mail": "workspace:*",
    "@bulkit/redis": "workspace:*",
    "@bulkit/shared": "workspace:*",
    "@bogeychan/elysia-logger": "^0.1.2",
    "@elysiajs/cors": "^1.1.0",
    "@elysiajs/swagger": "^1.1.1",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@paralleldrive/cuid2": "paralleldrive/cuid2",
    "@react-email/render": "^1.0.1",
    "@sinclair/typebox": "0.32.34",
    "ai": "^3.3.41",
    "arctic": "^1.9.2",
    "drizzle-orm": "^0.33.0",
    "drizzle-typebox": "^0.1.1",
    "elysia": "^1.1.7",
    "elysia-ip": "^1.0.7",
    "flydrive": "^1.1.0",
    "geoip-lite": "^1.4.10",
    "google-auth-library": "^9.14.1",
    "googleapis": "^144.0.0",
    "lucia": "^3.2.0",
    "nodemailer": "^6.9.15",
    "open-graph-scraper": "^6.8.2",
    "oslo": "^1.2.1",
    "pino-pretty": "^11.2.2",
    "postgres": "^3.4.4",
    "pusher": "^5.2.0",
    "react": "^18",
    "resend": "^4.0.0",
    "twitter-api-v2": "^1.17.2",
    "ua-parser-js": "^1.0.39"
  },
  "devDependencies": {
    "@elysiajs/eden": "^1.1.2",
    "@types/bun": "latest",
    "@types/nodemailer": "^6.4.15",
    "@types/geoip-lite": "^1.4.4",
    "@types/ua-parser-js": "^0.7.39",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2"
  },
  "module": "src/index.js"
}
